{
  "name": "glitch-hello-react",
  "version": "0.0.1",
  "description": "A simple React single page app, built with Vite and React.",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "cy:open-e2e": "cypress open --e2e --browser electron",
    "cy:run-e2e": "cypress run --e2e",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e",
    "cy:run": "start-server-and-test start http-get://localhost:3000 cy:run-e2e",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.8",
    "react-router-dom": "^6.17.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cypress": "^13.3.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.1",
    "vite": "^2.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/forest-polished-guppy"
  },
  "glitch": {
    "projectType": "generated_static"
  }
}
